{
  "name": "GoldenFold",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "generators": {
    "@schematics/angular:component": {
      "standalone": false
    },
    "@schematics/angular:directive": {
      "standalone": false
    },
    "@schematics/angular:pipe": {
      "standalone": false
    }
  },
  "sourceRoot": "apps/GoldenFold/src",
  "prefix": "app",
  "targets": {
    "build": {
      "executor": "@angular-devkit/build-angular:application",
      "options": {
        "outputPath": "dist/apps/GoldenFold",
        "index": "apps/GoldenFold/src/index.html",
        "browser": "apps/GoldenFold/src/main.ts",
        "polyfills": ["zone.js"],
        "tsConfig": "apps/GoldenFold/tsconfig.app.json",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/GoldenFold/src/assets",
            "output": "/assets"
          },{
            "glob": "**/*",
            "input": "public/assets",
            "output": "/assets"
          },

          {
            "glob": "**/*",
            "input": "public/assets",
            "output": "/assets"
          },
          "apps/GoldenFold/src/silent-renew.html"
        ],
        "styles": [
          "apps/GoldenFold/src/styles.css",
          "node_modules/@angular/material/prebuilt-themes/indigo-pink.css"
        ],
        "scripts": []
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kB",
              "maximumError": "1MB"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kB",
              "maximumError": "4kB"
            }
          ],
          "outputHashing": "all"
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true
        }
      },
      "defaultConfiguration": "development"
    },
   "serve": {
  "executor": "@angular-devkit/build-angular:dev-server",
  "options": {
    "buildTarget": "GoldenFold:build:development",
    "port": 4201
  },
  "configurations": {
    "production": {
      "buildTarget": "GoldenFold:build:production",
      "port": 4201
    },
    "development": {
      "buildTarget": "GoldenFold:build:development",
      "port": 4201
    }
  }
},
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n"
    },
    "test": {
      "executor": "@angular-devkit/build-angular:karma",
      "options": {
        "polyfills": ["zone.js", "zone.js/testing"],
        "tsConfig": "apps/GoldenFold/tsconfig.spec.json",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/GoldenFold/public"
          }
        ],
        "styles": [
          "apps/GoldenFold/@angular/material/prebuilt-themes/azure-blue.css",
          "apps/GoldenFold/src/styles.css"
        ],
        "scripts": [],
        "karmaConfig": "apps/GoldenFold/karma.conf.js"
      }
    }
  }
}
 